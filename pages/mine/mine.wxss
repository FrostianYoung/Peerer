.tabs {
  border: none;
  border-radius: 10px;
  box-shadow: 0 2px 6px 0 rgb(0, 0, 0, 10%);
}
.content-body.tabs {
  width: 100%;
  margin-top: 1rem;
  background-color: #fff;
  font-size: 14px;
  text-align: center;
}
.content-body {
  box-shadow: 0 2px 6px rgba(0,0,0,10%);
  border-radius: 10px;
  line-height: 1.8rem;
  font-weight: 400;
  font-style: normal;
  padding-left: 1rem;
  padding-right: 1rem;
  padding-bottom: 1.5rem;
  box-sizing: border-box;
}
.content-line {
  text-align: left;
  border-bottom: 1px solid #f0f0f0;
  float: left;
  width: 100%;
  padding-top: 1rem;
}
.content-line>.icon{
  float: left;
  width: 36px;
  text-align: left;
  position: relative;
  top: -.2rem;
}
.content-line>.content{
  float: left;
  width: calc(100% - 2.5rem);
  padding-bottom: .3rem;
}
.content-line>.content>view{
  font-weight: 400;
  font-size: 14px;
  color: #444;
  font-style: normal;
  letter-spacing: 0;
  line-height: 22px;
  text-decoration: none;
  margin-bottom: 10px;
  float: left;
  width: 100%;
}
